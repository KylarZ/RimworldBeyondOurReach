<Defs>

  <PipeSystem.ProcessDef ParentName="BOR_ProcessGenIITemp">
    <defName>BOR_NeutroniumIntoSteelGenIITempRuin</defName>
    <label>Fabricate Steel from Neutronium within safe temperature (-15*C - 0*C)</label>
    <ingredients>
      <li>
        <pipeNet>BOR_NeutroniumNet</pipeNet>
        <thing>BOR_Neutronium</thing>
        <countNeeded>3000</countNeeded>
      </li>
      <li>
        <thing>Steel</thing>
        <countNeeded>1</countNeeded>
      </li>
    </ingredients>
    <ticks>600000</ticks>
    <results>
      <li>
        <thing>Steel</thing>
        <count>625</count>
      </li>
    </results>
  </PipeSystem.ProcessDef>


