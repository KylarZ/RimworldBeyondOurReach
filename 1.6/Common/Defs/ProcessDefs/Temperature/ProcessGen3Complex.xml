  <PipeSystem.ProcessDef ParentName="BOR_ProcessGenIIITemp">
    <defName>BOR_NeutroniumIntoComponentIndustrialGenIIITempRuin</defName>
    <label>Fabricate Component from Neutronium within safe temperature (-30*C - -15*C)</label>
    <ingredients>
      <li>
        <pipeNet>BOR_NeutroniumNet</pipeNet>
        <thing>BOR_Neutronium</thing>
        <countNeeded>4500</countNeeded>
      </li>
      <li>
        <thing>ComponentIndustrial</thing>
        <countNeeded>1</countNeeded>
      </li>
    </ingredients>
    <ticks>300000</ticks>
    <results>
      <li>
        <thing>ComponentIndustrial</thing>
        <count>375</count>
      </li>
    </results>
  </PipeSystem.ProcessDef>


